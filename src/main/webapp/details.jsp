						},
						traditional: true,
						dataType: "json",
						success: function(data) {
							console.log(data);
							//当数据传输出现错误时
							if (data == -1) {
								window.location.href = "details.jsp";
							}
							//当数据正确时
							po = data;
							showDetails(data);
						}
					});
				});

				//显示商品详情
				function showDetails(arr) {
					//pic img 设置详情图片
					$(".pic .img").html('');
					var str = '<img id="largePic" src="' + arr.gimg + '" width="320" height="320"/>';
					$(".pic .img").html(str);


					//name_info 商品名 和商品描述
					$(".name_info").html('');
					//商品名
					str = '<h1 title="' + arr.ganme + '">' +
						'<img src="images/icon_ddzy.png" class="icon_name">' + arr.gname + '</h1>';

					//商品描述
					str += '<h2><span class="head_title_name" title="' + arr.gdescribe + '">' +
						arr.gdescribe + '</span><span class="hot" title="' + arr.gdescribe + '>>&nbsp;">' +
						'</span><span class="yushou" id="presale-title"></span></h2>';
					$(".name_info").html(str);


					//dd-price 当当价设置
					$("#dd-price").html('');
					var gprice = parseInt(arr.gprice);
					str = '<span class="yen">&yen;</span>' + gprice.toFixed(2) + '                ';
					$("#dd-price").html(str);


					//original-price 定价
					$("#original-price").html('');
					var price = gprice + 5.0;
					str = '<span class="yen">&yen;</span>' + price.toFixed(2) + '            ';
					$("#original-price").html(str);
				}

				//增加购买商品数量
				function addVal() {
					//获取当前商品的数量 num_add
					var num = parseInt($('#buy-num').val());
					console.log(num);
					//判断商品是否等于 商品总数
					if (num >= po.gnum) {
						$('.num_add').addClass("num_disabled");
					} else {
						$('#buy-num').val(num + 1 + "");
					}

					$('.num_del').removeClass("num_disabled");
					changeVal();
				}

				//减少商品的购买
				function delVal() {
					//获取当前商品的数量 num_del
					var num = parseInt($('#buy-num').val());
					console.log(num);
					//判断商品是否为0 或 1 或大于 0
					if (num == 0) {
						$('.num_del').addClass("num_disabled");
					} else if (num == 1) {
						$('#buy-num').val(num - 1);
						$('.num_del').addClass("num_disabled");
					} else {
						$('#buy-num').val(num - 1);
					}
					$('.num_add').removeClass("num_disabled");
					changeVal();
				}


				function changeVal() {
					var num = parseInt($('#buy-num').val());
					if (num < 0) {
						$('#buy-num').val(0);
						num = 0;
					}
					//dd-price 当当价设置
					$("#dd-price").html('');
					var gprice = parseInt(po.gprice) * num;
					console.log(gprice);
					str = '<span class="yen">&yen;</span>' + gprice.toFixed(2) + '                ';
					$("#dd-price").html(str);


					//original-price 定价
					$("#original-price").html('');
					var price = (parseInt(po.gprice) + 5.0) * num;
					str = '<span class="yen">&yen;</span>' + price.toFixed(2) + '            ';
					$("#original-price").html(str);


				}

				function add_cart() {
					var num = parseInt($('#buy-num').val());
					$.post('favorite/addcart',{gno:po.gno,fnum:num},function(data){
						if(data==1){
							alert('添加成功');
						}else{
							alert('添加失败');
						}
					})
				}
			</script>

	</body>
</html>
